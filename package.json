{
	"name": "new-site",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"generate-types": "env-cmd --silent --no-override -x cf-content-types-generator -s '$C_SPACE' -t='$C_PAT' -e master -o src/lib/types/contentful -g -l"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.3",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-netlify": "^4.4.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"ai.robots.txt": "github:ai-robots-txt/ai.robots.txt",
		"cf-content-types-generator": "^2.15.5",
		"contentful": "^11.4.0",
		"env-cmd": "^10.1.0",
		"eslint": "^9.7.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"globals": "^15.0.0",
		"prettier": "^3.3.2",
		"prettier-plugin-svelte": "^3.2.6",
		"sass-embedded": "^1.83.1",
		"svelte": "^5.16.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.0.0",
		"vite": "^5.4.11"
	},
	"dependencies": {
		"@atproto/api": "^0.13.32",
		"@contentful/node-apps-toolkit": "^3.10.2",
		"@contentful/rich-text-types": "^17.0.0",
		"highlight.js": "^11.11.1",
		"image2uri": "^2.1.2",
		"piexifjs": "^1.0.6"
	}
}
